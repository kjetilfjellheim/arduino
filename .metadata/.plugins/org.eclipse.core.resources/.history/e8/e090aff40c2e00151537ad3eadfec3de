// Do not remove the include below
#include "Ultrasonic.h"
#include <Arduino.h>

int centimeters = 0;

//The setup function is called once at startup of the sketch
void setup()
{
	Serial.begin(9600);
}

// The loop function is called in an endless loop
void loop()
{
	  Serial.print(centimeters);
	  Serial.print("cm");
	  Serial.println();
}
